{
    "name": "vstelmakh/twig-url-highlight",
    "description": "Twig filter to convert urls to html tags",
    "keywords": ["twig", "extension", "highlight", "url", "html"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "twig/twig": "^1.4|^2|^3"
    },
    "require-dev" : {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "VStelmakh\\Twig\\Extension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VStelmakh\\Twig\\Extension\\Tests\\": "tests/"
        }
    }
}
